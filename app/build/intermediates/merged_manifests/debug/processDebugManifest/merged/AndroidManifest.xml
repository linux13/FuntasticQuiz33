<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell_pc.funtasticquiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/quiz"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/quiz"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.dell_pc.funtasticquiz.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dell_pc.funtasticquiz.Main2Activity"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.Start"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.Instruction"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.About"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.Exit"
            android:theme="@style/MyAppTheme" />
        <activity android:name="com.example.dell_pc.funtasticquiz.Simple" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.FinalResult"
            android:theme="@style/MyAppTheme" />
        <activity android:name="com.example.dell_pc.funtasticquiz.SSound" />
        <activity android:name="com.example.dell_pc.funtasticquiz.VVideo" />
        <activity android:name="com.example.dell_pc.funtasticquiz.IImage" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.ResultSplash"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name="com.example.dell_pc.funtasticquiz.FinalResultFailSplash"
            android:theme="@style/MyAppTheme" />
    </application>

</manifest>