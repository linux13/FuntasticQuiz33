<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell_pc.funtasticquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/quiz"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" android:theme="@style/MyAppTheme"/>
        <activity android:name=".Start" android:theme="@style/MyAppTheme"/>
        <activity android:name=".Instruction" android:theme="@style/MyAppTheme"/>
        <activity android:name=".About" android:theme="@style/MyAppTheme"/>
        <activity android:name=".Exit" android:theme="@style/MyAppTheme" />
        <activity android:name=".Simple" />
        <activity android:name=".FinalResult" android:theme="@style/MyAppTheme"/>
        <activity android:name=".SSound" />
        <activity android:name=".VVideo" />
        <activity android:name=".IImage" />
        <activity android:name=".ResultSplash" android:theme="@style/MyAppTheme"/>
        <activity android:name=".FinalResultFailSplash" android:theme="@style/MyAppTheme"></activity>
    </application>

</manifest>